<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aqib.icrave">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".controller.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".controller.ImageActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".controller.RootActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".controller.ResultActivity"
            android:label="@string/app_name"
            android:parentActivityName=".controller.RootActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="RootActivity" />
        </activity>
        <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    </application>



</manifest>
