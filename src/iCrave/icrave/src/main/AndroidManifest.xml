<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aqib.icrave">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.aqib.icrave.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.aqib.icrave.ImageActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name="com.aqib.icrave.RootActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.aqib.icrave.ResultActivity"
            android:label="@string/title_activity_result"
            android:parentActivityName="com.aqib.icrave.RootActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="RootActivity" />
        </activity>
    </application>

</manifest>
